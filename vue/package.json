{
  "name": "master-tailwind",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "build-and-move": "powershell -Command \"Remove-Item ../myapp/dist -Recurse -Force; vite build; Copy-Item -Path dist -Destination ../myapp -Recurse\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.14",
    "pinia": "^3.0.1",
    "tailwindcss": "^4.0.14",
    "three": "^0.175.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.15.13",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.13",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "~5.8.0",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
